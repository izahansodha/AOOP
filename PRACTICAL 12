class Shape {
    
    Shape(float radius) {
        System.out.println("The area of circle: " + (Math.PI * radius * radius));
    }

    
    Shape(float length, float width) {
        System.out.println("The area of rectangle: " + (length * width));
    }
}

public class ConstructorOverloadingDemo {
    public static void main(String[] args) {
        Shape s1 = new Shape(10);   
        Shape s2 = new Shape(5, 2); 
    }
}
