public class Rectangle {
    double height;
    double weight;

    
    public Rectangle(double h, double w) {
        height = h;
        weight = w;
    }

    public static void main(String[] args) {
        Rectangle r1 = new Rectangle(6.2, 12.5);
        System.out.println(r1.height);
        System.out.println(r1.weight);
    }
}
