class Shape {
    
    public void area(float radius) {
        System.out.println("The area of circle: " + (Math.PI * radius * radius));
    }

    
    public void area(float length, float width) {
        System.out.println("The area of rectangle: " + (length * width));
    }
}

public class Main {
    public static void main(String[] args) {
        Shape s1 = new Shape();
        s1.area(5);       
        s1.area(5, 10);  
    }
}
