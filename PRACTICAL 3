public class ReversedNumber {
    public static void main(String[] args) {
        int reversednum=0;
        int num=123;
        while (num > 0) {
            int digit = num % 10; 
            reversedNum = reversedNum * 10 + digit; 
            num /= 10; 
        }

        
        System.out.println("Reversed Number: " + reversedNum);
    }
}
