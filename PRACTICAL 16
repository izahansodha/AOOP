class Animal {
    public void Eating() {
        System.out.println("Eating...");
    }
}


class Dog extends Animal {
    public void DogInfo() {
        System.out.println("This is Dog.");
    }
}


class Cat extends Animal {
    public void CatInfo() {
        System.out.println("This is Cat.");
    }
}


public class Main {
    public static void main(String[] args) {
        Dog d1 = new Dog();
        d1.Eating();   
        d1.DogInfo();  
        
        Cat c1 = new Cat();
        c1.Eating();  
        c1.CatInfo();  
    }
}
